package com.springcore.autowire.annotation;

import org.springframework.beans.factory.annotation.Autowired;

public class Student {
	@Autowired
	private int roll;
	private Person p1;
	public int getRoll() {
		return roll;
	}
	public void setRoll(int roll) {
		this.roll = roll;
	}
	public Person getP1() {
		return p1;
	}
	public void setP1(Person p1) {
		this.p1 = p1;
	}
	public Student(int roll, Person p1) {
		super();
		this.roll = roll;
		this.p1 = p1;
	}
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "Student [roll=" + roll + ", p1=" + p1 + "]";
	}

}
