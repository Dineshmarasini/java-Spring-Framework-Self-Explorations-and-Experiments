<!--Using Value as aattributes -->

<bean class="com.springcore1.Student" name="student2">
    <property name="studentId" value="234"/>
         

    <property name="studentName" value="Dinesh Marasini"/>
         

    <property name="studentAddress" value="Gulmi"/>
         

</bean>

<!-- Using p schema  -->

<bean class="com.springcore1.Student" name="student2" p:studentId="123" p:studentName="marasini dinesh" p:studentAddress="Thulacheckmi" />



</beans>

